{
  "name": "inkray-sdk-scripts",
  "version": "1.0.0",
  "description": "TypeScript SDK scripts for Inkray decentralized blogging platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "deploy": "tsx src/deployment/deploy.ts",
    "setup-seal": "tsx src/deployment/setup-seal.ts",
    "verify": "tsx src/deployment/verify.ts",
    "upload-free": "tsx src/workflows/publish-free.ts",
    "upload-paid": "tsx src/workflows/publish-paid.ts",
    "create-publication": "tsx src/interactions/publication.ts",
    "create-subscription": "tsx src/interactions/subscriptions.ts",
    "mint-nft": "tsx src/interactions/nft.ts",
    "demo:creator": "tsx src/examples/creator-journey.ts",
    "demo:reader": "tsx src/examples/reader-journey.ts",
    "demo:platform": "tsx src/examples/full-platform.ts",
    "test:seal": "tsx src/simple-decrypt-test.ts"
  },
  "keywords": [
    "sui",
    "blockchain",
    "walrus",
    "seal",
    "decentralized",
    "blogging",
    "inkray"
  ],
  "author": "Inkray Team",
  "license": "MIT",
  "dependencies": {
    "@mysten/bcs": "^1.6.0",
    "@mysten/dapp-kit": "^0.16.13",
    "@mysten/seal": "0.4.14",
    "@mysten/sui": "^1.34.0",
    "@mysten/walrus": "latest",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@web-std/file": "^3.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.8.2",
    "undici": "^7.13.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}